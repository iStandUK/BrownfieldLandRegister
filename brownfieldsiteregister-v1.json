{
  "title" : "Brownfield Site Register - 2017 Regulations",
  "fields": [
    {
      "name": "OrganisationURI",
      "title": "OrganisationURI",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#anyURI",
        "pattern": "^http:\\/\\/opendatacommunities.org\\/id\\/"
      }
    },
    {
      "name": "OrganisationLabel",
      "title": "OrganisationLabel",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "OrganisationType",
      "title": "OrganisationType",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(District|London Borough|Metropolitan District|County|Broads Authority|National Park Authority|Mayoral development corporation|Homes and Community Agency)"
      }
    },
    {
      "name": "SiteReference",
      "title": "SiteReference",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "SiteName_Address",
      "title": "SiteName_Address",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "SiteplanURL",
      "title": "SiteplanURL",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#anyURI",
        "pattern": "^(http|https):\\/\\/"
      }
    },
    {
      "name": "UPRN",
      "title": "UPRN",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(^[0-9]{12}$)?"
      }
    },
    {
      "name": "CoordinateReferenceSystem",
      "title": "CoordinateReferenceSystem",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(WGS84|OSGB36|ETRS89)"
      }
    },
    {
      "name": "GeoX",
      "title": "GeoX",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#float",
        "pattern": "^[+-]?\\d*\\.?\\d*$"
      }
    },
    {
      "name": "GeoY",
      "title": "GeoY",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#float",
        "pattern": "^[+-]?\\d*\\.?\\d*$"
      }
    },
    {
      "name": "Hectares",
      "title": "Hectares",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#float",
        "pattern": "^\\d*(\\.\\d{1,2})?$"
      }
    },
    {
      "name": "OwnershipStatus",
      "title": "OwnershipStatus",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(public ownership|not in public ownership|unknown ownership|mixed ownership)"
      }
    },
    {
      "name": "Owners",
      "title": "Owners",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "Deliverable",
      "title": "Deliverable",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(yes|no)"
      }
    },
    {
      "name": "Constraints",
      "title": "Constraints",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#anyURI",
        "pattern": "(^(http|https):\\/\\/)?"
      }
    },
    {
      "name": "PlanningStatus",
      "title": "PlanningStatus",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(permissioned|not permissioned|pending decision)"
      }
    },
    {
      "name": "PermissionType",
      "title": "PermissionType",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(Full planning permission|Outline planning permission|Reserved matters approval|Permission in principle|Technical details consent|Planning permission granted under a local development order or neighbour development order|other)?"
      }
    },
    {
      "name": "PermissionDate",
      "title": "PermissionDate",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(^(?:(?:31(\\/)(?:0?[13578]|1[02]))\\1|(?:(?:29|30)(\\/)(?:0?[1,3-9]|1[0-2])\\2))(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$|^(?:29(\\/)0?2\\3(?:(?:(?:1[6-9]|[2-9]\\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\\d|2[0-8])(\\/)(?:(?:0?[1-9])|(?:1[0-2]))\\4(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$)?"
      }
    },
    {
      "name": "PlanningHistory",
      "title": "PlanningHistory",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#anyURI",
        "pattern": "(^(http|https):\\/\\/)?"
      }
    },
    {
      "name": "ProposedforPIP",
      "title": "ProposedforPIP",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(yes|no)"
      }
    },
    {
      "name": "AllocatedforPIP",
      "title": "AllocatedforPIP",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "(yes|no)"
      }
    },
    {
      "name": "DevelopmentDescription",
      "title": "DevelopmentDescription",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "NetHomes",
      "title": "NetHomes",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "MinHomes",
      "title": "MinHomes",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#float"
      }
    },
    {
      "name": "Events",
      "title": "Events",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "Notes",
      "title": "Notes",
      "constraints": {
        "required": false,
        "type": "http://www.w3.org/2001/XMLSchema#string"
      }
    },
    {
      "name": "FirstAddedDate",
      "title": "FirstAddedDate",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "^(?:(?:31(\\/)(?:0?[13578]|1[02]))\\1|(?:(?:29|30)(\\/)(?:0?[1,3-9]|1[0-2])\\2))(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$|^(?:29(\\/)0?2\\3(?:(?:(?:1[6-9]|[2-9]\\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\\d|2[0-8])(\\/)(?:(?:0?[1-9])|(?:1[0-2]))\\4(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$"
      }
    },
    {
      "name": "LastUpdatedDate",
      "title": "LastUpdatedDate",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#string",
        "pattern": "^(?:(?:31(\\/)(?:0?[13578]|1[02]))\\1|(?:(?:29|30)(\\/)(?:0?[1,3-9]|1[0-2])\\2))(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$|^(?:29(\\/)0?2\\3(?:(?:(?:1[6-9]|[2-9]\\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\\d|2[0-8])(\\/)(?:(?:0?[1-9])|(?:1[0-2]))\\4(?:(?:1[6-9]|[2-9]\\d)?\\d{2})$"
      }
    }
  ]
}